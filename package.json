{
  "name": "fastify-get-head",
  "version": "3.0.0",
  "description": "Set a HEAD route handler for each GET handler previously registered",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "standard | snazzy",
    "lint:ci": "standard",
    "release": "npx standard-version",
    "test": "tap test/*.test.js",
    "test:ci": "npm test && npm run test:ts && npm run lint:ci",
    "test:coverage": "npm test -- --cov --coverage-report=html",
    "test:only": "tap test/*.test.js --only",
    "test:ts": "tsd"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "fastify",
    "fastify-get-head",
    "http",
    "get",
    "head"
  ],
  "author": "Carlos Fuentes <carlos.fuentes.rios.95@gmail.com>",
  "repository": {
    "url": "https://github.com/MetCoder95/fastify-get-head"
  },
  "bugs": {
    "url": "https://github.com/MetCoder95/fastify-get-head/issues"
  },
  "homepage": "https://github.com/MetCoder95/fastify-get-head",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20",
    "fastify": "^3.9.2",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "standard": "^16.0.1",
    "tap": "^16.3.4",
    "tsd": "^0.13.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0"
  },
  "tsd": {
    "directory": "test"
  },
  "precommit": [
    "lint",
    "test"
  ]
}
